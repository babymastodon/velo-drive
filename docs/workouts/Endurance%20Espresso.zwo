      <textevent timeoffset="675" duration="10" message="A quick crest to ~90%—keep the pull smooth and unhurried." />
    </SteadyState>
    <SteadyState Duration="60" Power="0.90" Cadence="96" />
    <SteadyState Duration="120" Power="0.70" Cadence="90" />
    <SteadyState Duration="180" Power="0.65" Cadence="88">
    </SteadyState>
    <SteadyState Duration="120" Power="0.75" Cadence="94" />
